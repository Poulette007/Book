FONCTION calculFacto(valeur) : resulta
	//calculer factoriel d'un nombre donner
	//valeur (entier) : valeur a calculer 
	//resulta (entier) : valeur resulta
DEBUT
	resulta := 1
	POUR i := 1 A valeur
		resulta := resulta*i
	retourner resulta
FIN

